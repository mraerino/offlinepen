{
  "entrypoint": "index.html",
  "shell": "src/op-app.html",
  "fragments": [
    "src/view-list.html",
    "src/view-editor.html"
  ],
  "sources": [
    "src/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "_redirects",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
